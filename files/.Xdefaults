!-------------------------
! URXVT Terminal config
! --------------------------
URxvt*buffered: true
URxvt*font: xft:MesloLGM:size=9:antialias=true
URxvt.termName: rxvt-256color
URxvt.letterSpace: -1
URxvt.transparent: true
URxvt.shading: 25
URxvt.scrollBar: false
URxvt.urlLauncher: chromium
URxvt.foreground: #eeeeee
URxvt.background: #000000
URxvt.utf8: true
URxvt.locale: true

!! black dark/light
*color0: #282a36
*color8: #282a36

!! red dark/light
*color1: #ff625c
*color9: #ff625c

!! green dark/light
*color2: #63fb8e
*color10: #63fb8e

!! yellow dark/light
*color3: #f4fb9e
*color11: #f4fb9e

!! blue dark/light
*color4: #c9a4fa
*color12: #c9a4fa

!! magenta dark/light
*color5: #fd8acf
*color13: #fd8acf

!! cyan dark/light
*color6: #9dedfd
*color14: #9dedfd

!! white dark/light
*color7: #f9f9f5
*color15: #f9f9f5
